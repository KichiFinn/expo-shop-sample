{
  "expo": {
    "name": "ExpoShop",
    "slug": "ExpoShop",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.sample.ExpoShop",
      "supportsTablet": true
    },
    "android": {
      "package": "com.sample.ExpoShop"
    }
  }
}
